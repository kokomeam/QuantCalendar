{
  "compilerOptions": {
    "target": "ES2023",
    "lib": ["ES2023"],
    "module": "ESNext",
    "moduleResolution": "node",
    "types": ["node"],
    "skipLibCheck": true,
    "strict": true,
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "outDir": "./dist/backend",
    "rootDir": "./src/backend"
  },
  "include": ["src/backend/**/*"],
  "exclude": ["node_modules"]
}
